{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-prototype-builtins": "off",
    "linebreak-style": ["error", "unix"],
    "semi": ["error", "always"]
  }
}
